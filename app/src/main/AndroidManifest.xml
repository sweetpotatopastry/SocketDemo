<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yl.bamai.com.socketclient">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:name="yl.bamai.com.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="yl.bamai.com.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.ChatClientMain"></activity>
        <activity android:name="yl.bamai.com.socketService.ui.ChatUI"></activity>
    </application>

</manifest>